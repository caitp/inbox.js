{
  "preset": "google",
  "maximumLineLength": {
    "value": 200,
    "allowComments": true,
    "allowRegex": true
  },
  "disallowSpacesInsideObjectBrackets": null,
  "disallowSpacesInsideArrayBrackets": null,
  "disallowMultipleLineBreaks": null,
  "requireCurlyBraces": [
    "else",
    "for",
    "while",
    "do",
    "try",
    "catch"
  ]
}
