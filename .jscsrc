{
  "preset": "google",
  "maximumLineLength": {
    "value": 100,
    "allowComments": true,
    "allowRegex": true
  },
  "disallowSpacesInsideObjectBrackets": null,
  "disallowSpacesInsideArrayBrackets": null,
  "disallowMultipleLineBreaks": null,
  "requireCurlyBraces": [
    "else",
    "for",
    "while",
    "do",
    "try",
    "catch",
    "case",
    "default"
  ],
  "validateJSDoc": null
}
